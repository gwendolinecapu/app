      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.plural.connect",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-local-authentication",
      [
        "expo-sqlite",
        {
          "enableFTS": true
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-7014088517639318~1112284921",
          "androidAppId": "ca-app-pub-7014088517639318~7514041741",
          "delayAppMeasurementInit": true
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "L'accès à vos photos est nécessaire pour les ajouter à la galerie de l'alter."
        }
      ],
      "@react-native-google-signin/google-signin",
      "./plugins/withUnityAds",
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "./plugins/withModularHeaders"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
